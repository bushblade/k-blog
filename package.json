{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "postinstall": "remix setup node",
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config"
  },
  "dependencies": {
    "@graphcms/rich-text-react-renderer": "^0.5.0",
    "@remix-run/react": "^1.6.2",
    "@remix-run/serve": "^1.6.2",
    "@remix-run/vercel": "^1.6.2",
    "daisyui": "^2.17.0",
    "dotenv": "^16.0.1",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix": "^1.6.2"
  },
  "devDependencies": {
    "@graphcms/rich-text-types": "^0.4.0",
    "@graphql-codegen/cli": "2.6.3",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.5.1",
    "@graphql-codegen/typescript-operations": "2.4.2",
    "@graphql-codegen/typescript-resolvers": "2.6.6",
    "@remix-run/dev": "^1.6.2",
    "@remix-run/eslint-config": "^1.6.2",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "concurrently": "^7.2.2",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
