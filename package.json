    "@remix-run/react": "^1.7.2",
    "@remix-run/serve": "^1.7.2",
    "@remix-run/vercel": "^1.7.2",
    "daisyui": "^2.31.0",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-utils": "^3.3.0",
    "three": "^0.145.0"
  },
  "devDependencies": {
    "@graphcms/rich-text-types": "^0.5.0",
